<Window x:Class="BankFilteringSystem.Front_end.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankFilteringSystem.Front_end"
        mc:Ignorable="d"
        Title="Import" MaxHeight="500" MaxWidth="800" MinHeight="500" MinWidth="800">
    <Grid>
        <Button x:Name="btnImport" Content="Import Messages" HorizontalAlignment="Left" Margin="45,0,0,0" VerticalAlignment="Center" Height="199" Width="264" Click="Button_Click" FontSize="20" FontFamily="Arial Black"/>
        <Button x:Name="btnExit" Content="Return" HorizontalAlignment="Left" Margin="449,0,0,0" VerticalAlignment="Center" Height="199" Width="264" FontSize="20" FontFamily="Arial Black" Click="BtnExit_Click"/>

    </Grid>
</Window>
